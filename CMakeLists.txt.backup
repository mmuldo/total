# Generated Cmake Pico project file

cmake_minimum_required(VERSION 3.13)

set(CMAKE_C_STANDARD 11)
set(CMAKE_CXX_STANDARD 17)

# initalize pico_sdk from installed location
# (note this can come from environment, CMake cache etc)
# set(PICO_SDK_PATH "/YOUR_PICO_SDK_PATH/pico-sdk")

# Pull in Pico SDK (must be before project)
include(pico_sdk_import.cmake)

project(adc C CXX ASM)

set(PICO_CXX_ENABLE_EXCEPTIONS 1)

set(PICO_CXX_ENABLE_RTTI 1)

# Initialise the Pico SDK
pico_sdk_init()

# Add executable. Default name is the project name, version 0.1

add_executable(adc adc.c )

pico_set_program_name(adc "adc")
pico_set_program_version(adc "0.1")

pico_enable_stdio_uart(adc 0)
pico_enable_stdio_usb(adc 1)

# Add the standard library to the build
target_link_libraries(adc pico_stdlib pico_multicore)

# Add any user requested libraries
target_link_libraries(adc
        hardware_dma
        hardware_pio
        hardware_timer
        hardware_clocks
        hardware_pwm
        hardware_adc
        hardware_dma
        hardware_watchdog
        )

pico_add_extra_outputs(adc)

